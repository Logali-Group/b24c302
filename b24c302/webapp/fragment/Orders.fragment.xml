<core:FragmentDefinition
            xmlns="sap.m"
            xmlns:core="sap.ui.core">

    <Dialog>
    
        <content>
            <Table items="{jsonEmployees>Orders}">

                    <columns>

                        <Column>
                            <Label text="{i18n>OrderID}"/>
                        </Column>

                        <Column>
                            <Label text="{i18n>Freight}"/>
                        </Column>

                        <Column>
                            <Label text="{i18n>ShipAddress}"/>
                        </Column>
                    
                    </columns>

                    <items>

                        <ColumnListItem>

                            <cells>

                                <Text text="{jsonEmployees>OrderID}"/>
                                <Text text="{jsonEmployees>Freight}"/>
                                <Text text="{jsonEmployees>ShipAddress}"/>
                            
                            </cells>

                        </ColumnListItem>
                    
                    </items>

            </Table>
        </content>

    </Dialog>

</core:FragmentDefinition>