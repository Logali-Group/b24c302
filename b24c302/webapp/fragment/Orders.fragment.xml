<core:FragmentDefinition
            xmlns="sap.m"
            xmlns:core="sap.ui.core">

    <Dialog>
    
        <content>

            <core:Fragment fragmentName="b24c302.fragment.TableOrders"
                            type="XML"/>

        </content>

        <endButton>
            <Button id="cancel"
                    text="{i18n>cancel}"
                    type="Reject"
                    press="onCloseDialog"
                    width="6rem"/>
        </endButton>

        <beginButton>
            <Button id="navto"
                    text="{i18n>navto}"
                    type="Accept"
                    press="onNavToDetails"
                    width="6rem"/>
        </beginButton>

    </Dialog>

</core:FragmentDefinition>